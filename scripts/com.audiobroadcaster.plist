<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.audiobroadcaster</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/audiobroadcaster</string>
        <string>-port</string>
        <string>8080</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/usr/local/var/audiobroadcaster</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>
    <key>StandardErrorPath</key>
    <string>/usr/local/var/log/audiobroadcaster/error.log</string>
    <key>StandardOutPath</key>
    <string>/usr/local/var/log/audiobroadcaster/output.log</string>
    <key>KeepAlive</key>
    <true/>
    <key>RunAtLoad</key>
    <true/>
    <key>ThrottleInterval</key>
    <integer>30</integer>
</dict>
</plist>
